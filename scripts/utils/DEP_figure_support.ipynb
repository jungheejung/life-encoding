   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "from matplotlib.colors import LinearSegmentedColormap\n",
    "for key in ['Reds', 'Blues', 'Greens', 'Greys','Wistia', 'Purples']:\n",
    "# Define the custom red colormap\n",
    "    reds_cmap = plt.get_cmap(key)\n",
    "\n",
    "    # Generate a random 40x1400 matrix\n",
    "    matrix = np.random.rand(1, 9) \n",
    "\n",
    "\n",
    "    # Plot the matrix using the custom colormap\n",
    "    plt.figure(figsize=(9, 1))  # Adjust the figure size as needed\n",
    "    plt.imshow(matrix, cmap=reds_cmap, aspect='auto')\n",
    "\n",
    "    # Remove the axis for a cleaner look\n",
    "    plt.axis('off')\n",
    "    plt.savefig(f'{key}_colormap.svg', format='svg', bbox_inches='tight', transparent=True)\n",
    "    # Show the plot\n",
    "    plt.show()"
   ]
  }
